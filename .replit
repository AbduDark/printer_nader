modules = ["dotnet-8.0"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "build"

[[workflows.workflow]]
name = "build"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd printer_nader && dotnet build && echo \"Build completed successfully. Note: This is a Windows Forms application and cannot run GUI on Linux without additional setup.\""

[workflows.workflow.metadata]
outputType = "console"
